{
    "config": {
        "platform": {
            "php": "8.2.0"
        }
    },
    "require-dev": {
        "shipmonk/phpstan-rules": "^1.2",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-0": {
            "List": "src"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyze -c rules.neon --xdebug",
        "phpcbf": "vendor/bin/phpcbf --standard=PSR12 src tests",
        "phpcs": "vendor/bin/phpcs --standard=PSR12 src tests",
        "test": "vendor/bin/phpunit tests"
    }
}
